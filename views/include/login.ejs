<form action="/users/users/login" method="POST">
    <div class="control">
      <label for="email">Email</label>
      <input
        type="email"
        id="email"
        name="email"
        class="form-control"
        placeholder="Enter Email"
        required
      />
    </div>
    <div class="control">
      <label for="password">Password</label>
      <input
        type="password"
        id="password"
        name="password"
        class="form-control"
        placeholder="Enter Password"
        required
      />
    </div>
    <button type="submit" class="btn">Login</button>
</form>